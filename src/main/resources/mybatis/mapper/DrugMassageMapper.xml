<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.woniuxy.clinic.mapper.DrugMassageMapper" >  	
  	<insert id="insertDrugMapper">
  		insert into drug_massage(stock_id,drugmassage_dose,drugmassage_use,drugmassage_fqt,drugmassage_date,drugmassage_type) values(#{stock_id},#{drugmassage_dose},#{drugmassage_use}
